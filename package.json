{
  "name": "shell-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next typegen && next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint src --ext ts,tsx --cache",
    "type-check": "next typegen && tsc --project ./tsconfig.json --noEmit",
    "dependency-check": "depcruise src",
    "dependency-security-audit": "bun audit --audit-level=critical",
    "format:write": "prettier src --write",
    "format:check": "prettier src --check",
    "preinstall": "npx only-allow bun"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "@types/bun": "^1.3.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "eslint-config-prettier": "^10.1.8",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "nuqs": "^2.8.6",
    "prettier": "^3.7.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^25",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "^1.0.0",
    "dependency-cruiser": "^17.3.6",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^17.0.0",
    "lefthook": "^2.0.13",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "typescript-eslint": "^8.51.0"
  }
}
